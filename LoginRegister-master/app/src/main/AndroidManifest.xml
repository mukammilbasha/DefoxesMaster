<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mind.Defoxes">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".progress_dialog"></activity>
        <activity
            android:name=".SplashPage"
            android:theme="@style/AppTheme.FullScreen" />
        <activity
            android:name=".PhoneNo"
            android:theme="@style/AppTheme.FullScreen" />
        <activity
            android:name=".DescriptionPage"
            android:theme="@style/AppTheme.FullScreen" />
        <activity
            android:name=".ProfieCreatedpage"
            android:theme="@style/AppTheme.FullScreen" />
        <activity
            android:name=".ContactDetails"
            android:parentActivityName=".RegisterActivity"
            android:theme="@style/AppTheme.FullScreen" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.FullScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegisterActivity"
            android:theme="@style/AppTheme.FullScreen" />
        <activity
            android:name=".LeadDetails"
            android:parentActivityName=".Profile"
            android:theme="@style/AppTheme.FullScreen" />
        <activity
            android:name=".Profile"
            android:parentActivityName=".ResidentialDetails"
            android:theme="@style/AppTheme.FullScreen" />
        <activity
            android:name=".ResidentialDetails"
            android:parentActivityName=".ContactDetails"
            android:theme="@style/AppTheme.FullScreen" />
    </application>

</manifest>